@model TaxRegisterModel

<div class="row">
    <div class="col-12">
        <div class="row">
            <div class="col-sm-4">
                <div class="form-group-sm">
                    @Html.LabelFor(model => model.TaxRegisterName, new { @class = "font-weight-bold" }) :
                    @Html.DisplayFor(model => model.TaxRegisterName)
                </div>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function () {

    });

    // edit taxregister master.
    function fnEditMaster(taxRegisterId) {

        var model = { taxRegisterId: taxRegisterId };

        AjaxCommunication.CreateRequest(this.window, 'Get', '@Url.Action("EditTaxRegisterMaster", "TaxRegister")', 'html', model,
        function (data) {
            $('#modal-taxregister').empty();
            $('#modal-taxregister').html(data).modal('show');
            return false;
        }, null, true, null, false);
    }


</script>
